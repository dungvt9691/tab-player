<h4><b><%= t('.title') %></b></h4>
<div class="row">
  <div class="col-md-12">
    <div class="form-group row <%= 'has-error' if @setting.errors.has_key?(:google_app_id) %>">
      <%= f.label :google_app_id, class: 'control-label col-md-2 col-sm-4'  %>
      <div class="col-md-6 col-sm-8">
        <%= f.text_field :google_app_id, class: "form-control" %>
        <p class="help-block">
          <% if @setting.errors.has_key?(:google_app_id) %>
          <%= @setting.errors[:google_app_id].join('<br>').html_safe %>
          <% end %>
        </p>
      </div>
    </div>
    <div class="form-group row <%= 'has-error' if @setting.errors.has_key?(:google_app_secret) %>">
