        <%= f.text_field :google_api_key, class: "form-control" %>
        <p class="help-block">
          <% if @setting.errors.has_key?(:google_api_key) %>
          <%= @setting.errors[:google_api_key].join('<br>').html_safe %>
          <% end %>
        </p>
      </div>
    </div>
    <div class="form-group row <%= 'has-error' if @setting.errors.has_key?(:google_analytics_id) %>">
      <%= f.label :google_analytics_id, class: 'control-label col-md-2 col-sm-4'  %>
