        <p class="help-block">
          <% if @setting.errors.has_key?(:google_app_secret) %>
          <%= @setting.errors[:google_app_secret].join('<br>').html_safe %>
          <% end %>
        </p>
      </div>
    </div>
    <div class="form-group row <%= 'has-error' if @setting.errors.has_key?(:google_api_key) %>">
      <%= f.label :google_api_key, class: 'control-label col-md-2 col-sm-4'  %>
