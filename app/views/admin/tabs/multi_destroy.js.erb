$('body').css("cursor", 'auto')
$("#modal-confirm-destroy #btn-confirm").removeClass("disabled")
$("#modal-confirm-destroy #btn-cancel").prop("disabled", false)
<% if @errors.empty? %>
<% @destroyed_tabs.each do |tab| %>
showMessage("<%= t_scope(@params, 'success', title: tab.title).html_safe %>.", "success")
$('.modal#modal-confirm-destroy').modal('hide');
var row = $(".btn-destroy[data-tab-id=<%= tab.id %>]").parents('tr')
if($(".btn-destroy[data-tab-id=<%= tab.id %>]").parents('table').attr('id') == "table-unpublish-tabs")
{
  window.unpublish_tabs_table.fnDeleteRow(row[0])
  window.unpublish_selected_id = []
  $("#table-unpublish-tabs .check-one").prop("checked", false)
  $("#table-unpublish-tabs #check-all").prop("checked", false)
  unpublish_tabs_count = parseInt($('.unpublish-tabs-count').text()) - 1
  published_tabs_count = parseInt($('.published-tabs-count').text())
  $('.unpublish-tabs-count').text(unpublish_tabs_count)
}
else{
  window.published_tabs_table.fnDeleteRow(row[0])
  window.published_selected_id = []
  $("#table-published-tabs .check-one").prop("checked", false)
  $("#table-published-tabs #check-all").prop("checked", false)
  published_tabs_count = parseInt($('.published-tabs-count').text()) - 1
  unpublish_tabs_count = parseInt($('.unpublish-tabs-count').text())
  $('.published-tabs-count').text(published_tabs_count)
}
$('.object-count').text(published_tabs_count + unpublish_tabs_count)
<% end %>
<% else %>
showMessage("<%= render_value(@errors) %>", "error")
<% end %>
setTimeout(function(){
  showMultiBtnActions()
}, 200)