$("#panel-favorites .loading").fadeOut(0)
<% if @count > 0 %>
<% if @tabs.blank? %>
window.stopFavoriteLoading = true
<% else %>
$("#alert-no-tabs").remove()
<% @tabs.each do |tab| %>
$("#panel-favorites .favorite-tabs .loading").before("<%=j render 'tab', tab: tab %>")
<% end %>
<% end %>
<% else %>
$("#alert-no-tabs").remove()
$("#panel-favorites .favorite-tabs").append("<%=j render 'no_tabs' %>")
<% end %>